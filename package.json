{
    "name": "kwatafana-node",
    "version": "0.1.0",
    "description": "kwatafana client",
    "main": "./dist/bundle.min.js",
    "homepage": "https://github.com/kwatafana/node",
    "keywords": [
        "peerjs",
        "webrtc",
        "p2p",
        "rtc"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kwatafana/node.git"
    },
    "bugs": {
        "url": "https://github.com/kwatafana/node/issues"
    },
    "files": [
        "dist/*.js",
        "dist/*.map",
        "lib/",
        "index.d.ts"
    ],
    "author": "Jackson Kaindume",
    "maintainers": [
        "Jackson Kaindume"
    ],
    "license": "MIT",
    "scripts": {
        "export": "parcel build exports.ts --out-file ./dist/kwatafana.min.js",
        "dev": "parcel build app/index.ts --out-file ./dist/kwatafana.min.js --no-minify",
	"rel": "parcel build app/index.ts --out-file ./dist/kwatafana.min.js",
        "build-nonminified": "parcel build peer/exports.ts --out-file ./dist/kwatafana.js --no-minify",
        "prepublishOnly": "npm run build",
        "start": "~/nwjs/nw dist/",
        "test": "mocha -r ts-node/register -r jsdom-global/register test/**/*.ts"
    },
    "release": {
        "branch": "root"
    },
    "types": "./index.d.ts",
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "chai": "^4.2.0",
        "jsdom": "^16.2.2",
        "jsdom-global": "^3.0.2",
        "mocha": "^7.1.2",
        "mock-socket": "8.0.5",
        "parcel": "1.12.3",
        "parcel-bundler": "^1.12.4",
        "standard": "^14.1.0",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@types/node": "^10.14.17",
        "@types/nw.js": "^0.13.11",
        "eventemitter3": "^3.1.2",
        "peerjs-js-binarypack": "1.0.1",
        "webrtc-adapter": "^7.3.0"
    }
}
